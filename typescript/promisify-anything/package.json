{
  "name": "promisify-anything",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\"",
    "verify": "npm run format -- --check && npm run lint && npm run build",
    "client": "ts-node src/client.ts",
    "service": "ts-node src/service.ts",
    "service:dev": "RESTATE_DEBUG_LOGGING=JOURNAL ts-node-dev --respawn --transpile-only src/service.ts",
    "deploy": "npx cdk deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-athena": "^3.504.0",
    "@restatedev/restate-sdk": "^0.7.0",
    "@types/uuid": "^9.0.8",
    "axios-retry": "^4.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@aws-cdk/aws-glue-alpha": "^2.122.0-alpha.0",
    "aws-cdk-lib": "^2.121.1",
    "cdk": "^2.121.1",
    "constructs": "^10.3.0",
    "prettier": "^3.2.4",
    "ts-node-dev": "^2.0.0"
  }
}
